{
  "name": "beeartena-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ci": "next lint --config .eslintrc.ci.json",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "firebase:setup": "./firebase-setup-existing.sh",
    "firebase:setup:new": "./firebase-setup.sh",
    "firebase:init": "tsx scripts/initializeFirebase.ts",
    "firebase:init:sample": "tsx scripts/initializeFirebase.ts --sample",
    "firebase:check": "tsx scripts/checkFirebaseData.ts",
    "firebase:deploy": "firebase deploy",
    "vercel:deploy": "vercel",
    "vercel:deploy:prod": "vercel --prod",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__/unit --passWithNoTests",
    "test:integration": "jest --testPathPattern=__tests__/integration --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:smoke": "playwright test --project=smoke-chromium",
    "test:e2e:critical": "playwright test --project=critical-chromium",
    "test:e2e:visual": "playwright test --project=visual",
    "test:e2e:a11y": "playwright test --project=a11y",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright test --project=critical-chromium --reporter=junit",
    "test:perf": "echo 'Performance tests skipped'",
    "test:a11y": "jest --testPathPattern=__tests__/accessibility --passWithNoTests",
    "test:all": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "ci:install": "npm ci --prefer-offline --no-audit",
    "ci:test": "npm run lint:ci && npm run type-check && npm run test:coverage",
    "ci:build": "npm run build",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out coverage playwright-report test-results",
    "precommit": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.14",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^12.7.0",
    "framer-motion": "^12.23.6",
    "googleapis": "^154.0.0",
    "ioredis": "^5.3.2",
    "jose": "^5.2.3",
    "lz-string": "^1.5.0",
    "moment": "^2.30.1",
    "next": "^15.4.1",
    "openapi-types": "^12.1.3",
    "postcss": "^8",
    "react": "^18",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "recharts": "^3.1.0",
    "resend": "^4.7.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react-big-calendar": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "axe-playwright": "^2.0.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^10.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.11",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "vitest": "^1.1.0"
  }
}
